{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "env": {"node": true},
    "rules": {
        "indent": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/indent": "off",
    }

}